<Window x:Class="OdinWPF.Tracker.TrackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OdinWPF.Tracker"
        
        mc:Ignorable="d"
        Title="TrackWindow" Height="450" Width="800">
    <Canvas x:Name="canv">
        <StackPanel x:Name="namesHolder">
            <TextBlock x:Name="procName" Text="procName"/>
            <TextBlock x:Name="screenName" Text="screenName"/>
        </StackPanel>
    </Canvas>
</Window>
